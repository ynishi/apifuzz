{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "SchemathesisOutput",
  "description": "Top-level output from the Python wrapper.",
  "type": "object",
  "properties": {
    "errors": {
      "description": "Errors from the wrapper itself (connection errors, etc.)",
      "type": "array",
      "default": [],
      "items": {
        "type": "string"
      }
    },
    "failure_count": {
      "description": "Number of failures",
      "type": "integer",
      "format": "uint64",
      "minimum": 0
    },
    "failures": {
      "description": "Raw failures from Schemathesis checks",
      "type": "array",
      "items": {
        "$ref": "#/$defs/RawFailure"
      }
    },
    "interactions": {
      "description": "Raw interactions (case + response pairs)",
      "type": "array",
      "default": [],
      "items": {
        "$ref": "#/$defs/RawInteraction"
      }
    },
    "success": {
      "description": "Successful requests (no check failures)",
      "type": "integer",
      "format": "uint64",
      "minimum": 0
    },
    "total": {
      "description": "Total HTTP requests made",
      "type": "integer",
      "format": "uint64",
      "minimum": 0
    }
  },
  "required": [
    "total",
    "success",
    "failure_count",
    "failures"
  ],
  "$defs": {
    "RawCase": {
      "description": "A test case (request) as produced by Schemathesis.",
      "type": "object",
      "properties": {
        "body": true,
        "headers": {
          "type": [
            "object",
            "null"
          ],
          "additionalProperties": {
            "type": "string"
          }
        },
        "id": {
          "type": [
            "string",
            "null"
          ],
          "default": null
        },
        "media_type": {
          "type": [
            "string",
            "null"
          ]
        },
        "method": {
          "type": "string"
        },
        "path": {
          "type": "string"
        },
        "path_parameters": {
          "type": [
            "object",
            "null"
          ],
          "additionalProperties": true
        },
        "query": {
          "type": [
            "object",
            "null"
          ],
          "additionalProperties": true
        }
      },
      "required": [
        "method",
        "path"
      ]
    },
    "RawFailure": {
      "description": "A failure from Schemathesis checks - mirrors schemathesis.checks.Failure hierarchy.\n\nSubtypes: ServerError, ResponseTimeExceeded, MalformedJson, CustomFailure, Failure (base)",
      "type": "object",
      "properties": {
        "case_id": {
          "description": "Case ID for reproduction",
          "type": [
            "string",
            "null"
          ]
        },
        "colno": {
          "description": "Column number",
          "type": [
            "integer",
            "null"
          ],
          "format": "uint64",
          "minimum": 0
        },
        "deadline": {
          "description": "Configured deadline in seconds",
          "type": [
            "number",
            "null"
          ],
          "format": "double"
        },
        "document": {
          "description": "The malformed document",
          "type": [
            "string",
            "null"
          ]
        },
        "elapsed": {
          "description": "Actual elapsed time in seconds",
          "type": [
            "number",
            "null"
          ],
          "format": "double"
        },
        "lineno": {
          "description": "Line number",
          "type": [
            "integer",
            "null"
          ],
          "format": "uint64",
          "minimum": 0
        },
        "message": {
          "description": "Detailed message",
          "type": "string"
        },
        "operation": {
          "description": "Operation label: \"GET /health\"",
          "type": "string"
        },
        "position": {
          "description": "Position in document",
          "type": [
            "integer",
            "null"
          ],
          "format": "uint64",
          "minimum": 0
        },
        "severity": {
          "description": "Schemathesis severity: \"critical\", \"high\", \"medium\", \"low\"",
          "type": "string"
        },
        "status_code": {
          "description": "HTTP status code (ServerError only)",
          "type": [
            "integer",
            "null"
          ],
          "format": "uint16",
          "maximum": 65535,
          "minimum": 0
        },
        "title": {
          "description": "Short title: \"Server error\", \"Response time limit exceeded\"",
          "type": "string"
        },
        "type": {
          "description": "Python class name: \"ServerError\", \"ResponseTimeExceeded\", \"MalformedJson\", etc.",
          "type": "string"
        },
        "validation_message": {
          "description": "JSON validation error message",
          "type": [
            "string",
            "null"
          ]
        }
      },
      "required": [
        "type",
        "operation",
        "title",
        "message",
        "severity"
      ]
    },
    "RawInteraction": {
      "description": "A single interaction: case + response + optional failures.",
      "type": "object",
      "properties": {
        "case": {
          "$ref": "#/$defs/RawCase"
        },
        "failures": {
          "type": "array",
          "default": [],
          "items": {
            "$ref": "#/$defs/RawFailure"
          }
        },
        "operation": {
          "type": "string"
        },
        "response": {
          "$ref": "#/$defs/RawResponse"
        }
      },
      "required": [
        "case",
        "response",
        "operation"
      ]
    },
    "RawResponse": {
      "description": "A response as seen by the fuzzer.",
      "type": "object",
      "properties": {
        "body": {
          "description": "Response body (truncated for large responses)",
          "type": [
            "string",
            "null"
          ]
        },
        "content_length": {
          "type": "integer",
          "format": "uint64",
          "default": 0,
          "minimum": 0
        },
        "elapsed": {
          "type": "number",
          "format": "double",
          "default": 0.0
        },
        "message": {
          "type": "string",
          "default": ""
        },
        "status_code": {
          "type": "integer",
          "format": "uint16",
          "maximum": 65535,
          "minimum": 0
        }
      },
      "required": [
        "status_code"
      ]
    }
  }
}